{
  "name": "n8n-nodes-opencode",
  "version": "0.1.0",
  "description": "n8n node to use OpenCode as an AI chat model",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "opencode",
    "ai",
    "chat"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ssccio/n8n-nodes-opencode",
  "author": {
    "name": "ssccio",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssccio/n8n-nodes-opencode.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts'",
    "lintfix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OpenCodeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OpenCode/OpenCode.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.41.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "*"
  }
}
